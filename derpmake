# babby's first makefile
inventory.o: inventory.c inventory.h
	cc -c inventory.c
main.o: main.c inventory.h 
	cc -c main.c
hacking: main.o inventory.o
	gcc -Wall -g main.c inventory.c -l ncurses -o hacking
test: main.o inventory.o
	gcc -Wall -g nodelaytest.c inventory.c -l ncurses -o hacking
clean:
	rm *.o ./hacking
